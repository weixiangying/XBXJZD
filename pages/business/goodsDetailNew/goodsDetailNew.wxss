.share-box{
    position: absolute;
    top: 24rpx;
    right: 24rpx;
    z-index: 30;
    width: 60rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    border-radius: 50%;
   margin: 0;
   padding: 0;
}
button[plain]{
    border: none;
    background-color: #fff;
} 
.share-icon{
    width: 36rpx;
    height: 34rpx;
}
.imgs-box{
    position: relative;
    width: 100%;
    height: 50vh;
}
.img-count-box{
    position: absolute;
    bottom: 24rpx;
    right: 24rpx;
    z-index: 10;
    font-size: 24rpx;
    color: #fff;
    text-align: center;
    background: rgba(0,0,0,.5);
    padding: 5rpx 10rpx;
    border-radius: 25rpx;
}
swiper{
    width: 100%;
    height: 100%;
}
swiper image{
    width: 100%;
    height: 100%;
    display: block;
}
.info-box{
    width: 100%;
    margin-bottom: 16rpx;
}
.info-bg-fff{
    width: 100%;
    background: #fff;
    padding: 0 24rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.point-box{
    width: 100%;
    padding: 24rpx 0;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
}
.point{
    font-size: 36rpx;
    color: #4bace2;
}
.hit{
    font-size: 24rpx;
    color: #666;
    text-align: right;
}
.title{
    width: 100%;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    font-size: 30rpx;
    color: #333;
    line-height: 1.5em;
    padding-bottom: 24rpx;
}
.info-tip{
    width: 100%;
    padding: 10rpx 24rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
    background: #fafafa;
}

.tip-icon{
    display: inline-block;
    vertical-align: middle;
    width: 20rpx;
    height: 20rpx;
    margin-right: 8rpx;
}
.tip-text{
    display: inline-block;
    vertical-align: middle;
    font-size: 24rpx;
    color: #666;
}


.h2{
    width: 100%;
    padding: 24rpx;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.h2-lt{
    display: flex;
    display: -webkit-flex;
    align-items: center;
}
.rect-icon{
    width: 8rpx;
    height: 30rpx;
    background: #4bace2;
    margin-right: 16rpx;
}
.h2-text{
    color: #333;
    font-size: 30rpx;
}
.h2-rt{
    width: 20%;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: flex-end;
}
.h2-rt-text{
    color: #999;
    font-size: 26rpx;
    margin-right: 10rpx;
    display: inline-block;
    vertical-align: middle;
}
.h2-rt-img{
    width: 14rpx;
    height: 22rpx;
    display: inline-block;
    vertical-align: middle;
}
.comment-box{
    width: 100%;
    background: #fff;
    margin-bottom: 16rpx;
}
.no-comment{
    width: 100%;
    font-size: 28rpx;
    color: #666;
    text-align: center;
    padding: 30rpx 0;
}
.comment-list{
    width: 100%;
    padding: 0 24rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.comment-item{
    width: 100%;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #dfdfdf;
}
.comment-item:first-child{
    padding-top: 0;
}
.comment-item:last-child{
    border-bottom: none;
}
.comment-info{
    width: 100%;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 16rpx;
}
.comment-lt{
    width: 60%;
}
.comment-user{
    width: 42rpx;
    height: 42rpx;
    margin-right: 16rpx;
    display: inline-block;
    vertical-align: middle;
    border-radius: 50%;
}
.comment-name{
    font-size: 26rpx;
    color: #666;
}
.comment-time{
    width: 40%;
    text-align: right;
    font-size: 24rpx;
    color: #999;
}
.comment-cont{
    width: 100%;
    font-size: 26rpx;
    color: #333;
    line-height: 1.5em;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    word-break: break-all;
    text-overflow: ellipsis;
}
.recommend-box{
    width: 100%;
    margin-bottom: 16rpx;
    background: #fff;
}

.scroll-box {
    width: 100%;
    white-space: nowrap; 
    padding: 24rpx; 
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.scroll-item {
    width: 30%;
    display: inline-block;
    vertical-align: top;
    margin-right: 5%;
    text-align: center;
}
.scroll-item:last-child{
    margin-right: 0;
}
.slide-image{
    width: 235rpx;
    height: 235rpx;
    background: #ececec;
    display: inline-block;
}
.gf-img{
    width: 90%;
    height: 97%;
    margin: 1.5% auto;
    display: block;
}
.slide-tit{
    width: 100%;
    padding: 0 5rpx;
    margin: 16rpx 0;
    font-size: 26rpx;
    color: #333;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    text-align: center;
}
.slide-point{
    width: 100%;
    padding: 0 5rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 26rpx;
    color: #4bace2;
    text-align: center;
}
.detail-box{
    width: 100%;
    background: #fff;
}
.wxParse{
    width: 100%;
    background: #fff;
    padding: 0 24rpx 24rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    overflow-x: hidden;
}
.wxParse-img{
    width: 100%;
    display: block;
}
.bottom-box {
    position: fixed;
    bottom: 0;
    z-index: 50;
    width: 100%;
    height: 10vh;
    background: #fff;
    border-top: 1rpx solid #dfdfdf;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.botm-cont{
    width: 94%;
    margin: 0 auto;
    padding: 2vh 0;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
}
.botm-btn {
    width: 50%;
    height: 6vh;
    line-height: 6vh;
    text-align: center;
    font-size: 28rpx;
    color: #fff;
    background: #4bace2;
    border-radius: 5rpx;
}

.collect,
.concat {
    width: 25%;
    height: 4vh;
    line-height: 4vh;
    text-align: center;
}
.concat{
    border-right: 1rpx solid #dfdfdf;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.botm-text{
    font-size: 28rpx;
    color: #666;
    display: inline-block;
    vertical-align: middle;
    margin: 0 16rpx;
}
.botm-icon{
    width: 36rpx;
    height: 38rpx;    
    display: inline-block;
    vertical-align: middle;
    margin-left: 16rpx;
}
.gift-box{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    width: 100%;
    height: 100vh;
    background: rgba(0, 0, 0, .5);
}
.gift-mask{
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
}
.gift-cont{
    position: relative;
    width: 65%;
    height: 50vh;
    margin: 25vh auto;
}
.bg-img{
    width: 100%;
    height: 100%;
    display: block;
}
.close-icon{
    position: absolute;
    top: 24rpx;
    right: 24rpx;
    width: 36rpx;
    height: 36rpx;
}
.gift-info-box{
    position: absolute;
    top: 20%;
    left: 10%;
    width: 80%;
}
.gift-img{
    width: 55%;
    margin: 0 auto 6vh;
    display: block;
}
.gift-name{
    width: 90%;
    font-size: 30rpx;
    color: #fff;
    text-align: center;
    margin: 0 auto 3vh;
    line-height: 1.5em;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.gift-btn{
    width: 70%;
    margin: 0 auto;
    display: block;
}
