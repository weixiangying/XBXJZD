page{
    padding-top:10vh;
}
.box{
    width: 100%;
    height: 90vh;
    background: #f7f7f7;
}
.header{
    width: 100%;
    margin: 16rpx 0;
    background: #fff;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: flex;
    display: -webkit-flex;
    align-items: center;
}
.h-img{
    width: 10vh;
    height: 10vh;
    margin-right: 5%;
}
.h-cont{
    max-width: 85%;
}
.tit-box {
    width: 100%;
    margin-bottom: 10rpx;
    overflow: hidden;
    display: flex;
    display: -webkit-flex;
    align-items: center;
}

.tit {
    font-size: 30rpx;
    color: #333;
    white-space: nowrap;
    margin-right: 5%;
}

.sex-icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 5%;
}


.sub {
    width: 100%;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 28rpx;
    color: #999;
}
.cont-box{
    width: 100%;
    background: #fff;
    margin-bottom: 16rpx;
}
.item{
    width: 100%;
    padding: 3% 30rpx;
    border-bottom: 1rpx solid #dfdfdf;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: flex;
    display: -webkit-flex;
    align-items: flex-start;
    justify-content: space-between;
}
.item:only-child{
    border-bottom: none;
}
.title{
    width: 20%;
    font-size: 30rpx;
    color: #333;
}
.cont{
    width: 80%;
    font-size: 30rpx;
    color: #21283f;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.next-icon{
    width: 17rpx;
    height: 26rpx;
}
.img-list{
    width: 80%;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
}
.img-list image{
    width: 32%;
    height: 23vw;
}
.btn{
    width: 90%;
    margin: 50rpx auto 0;
    background: #48b947;
    color: #fff;
    padding: 4% 0;
    border-radius: 10rpx;
    text-align: center;
    font-size: 28rpx;
}